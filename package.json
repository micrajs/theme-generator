{
  "name": "@micra/theme-generator",
  "version": "0.0.14-alpha.7",
  "author": "Olavo Amorim Santos<contact@micra.dev>",
  "license": "MIT",
  "main": ".micra/index.cjs.js",
  "module": ".micra/index.js",
  "types": ".micra/index.d.ts",
  "files": [
    ".micra/**/*",
    ".cli/**/*",
    "types.d.ts"
  ],
  "bin": {
    "theme-generator": ".cli/index.js"
  },
  "scripts": {
    "build:cli": "yarn rimraf ./.cli && node .config/build",
    "build:lib": "yarn rimraf .micra && yarn rollup -c .config/build/rollup.config.js && node .config/build/copyDTs",
    "build": "yarn build:lib && yarn build:cli",
    "format": "prettier --write 'src/**/*.{ts,tsx}' --config .prettierrc.js",
    "lint": "eslint 'src/**/*.{ts,tsx}' --cache --config .config/linting/.eslintrc.js --cache-location .config/linting/.eslintcache",
    "test": "jest --config .config/test/jest.config.js"
  },
  "devDependencies": {
    "@babel/core": "^7.12.13",
    "@babel/plugin-transform-react-jsx": "^7.12.13",
    "@babel/plugin-transform-typescript": "^7.12.13",
    "@babel/preset-env": "^7.12.13",
    "@types/cls-hooked": "^4.3.3",
    "@types/jest": "^26.0.20",
    "@types/lodash": "^4.14.168",
    "@types/mustache": "^4.1.1",
    "@types/node": "^14.14.33",
    "@types/nodemon": "^1.19.0",
    "@types/pluralize": "^0.0.29",
    "@types/prettier": "^2.2.1",
    "@types/react": "^17.0.1",
    "@types/styled-components": "^5.1.7",
    "@typescript-eslint/eslint-plugin": "^4.15.0",
    "@typescript-eslint/parser": "^4.15.0",
    "esbuild": "^0.9.0",
    "eslint": "^7.19.0",
    "eslint-config-prettier": "^7.2.0",
    "eslint-plugin-prettier": "^3.3.1",
    "fast-glob": "^3.2.5",
    "jest": "^26.6.3",
    "rimraf": "^3.0.2",
    "rollup": "^2.38.5",
    "rollup-plugin-babel": "^4.4.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-size-snapshot": "^0.12.0",
    "rollup-plugin-typescript2": "^0.29.0",
    "ts-jest": "^26.5.1",
    "ts-node": "^9.1.1"
  },
  "dependencies": {
    "@micra/application": "^0.0.14",
    "@micra/dot-env": "^0.0.3",
    "@micra/kernel": "^0.0.3",
    "@micra/path-match": "^0.0.6",
    "@micra/service-provider": "^0.0.2",
    "@micra/typedi-service-container": "^0.0.1",
    "cls-hooked": "^4.2.2",
    "cross-env": "^7.0.3",
    "lodash": "^4.17.21",
    "mustache": "^4.1.0",
    "nodemon": "^2.0.7",
    "pluralize": "^8.0.0",
    "polished": "^4.1.1",
    "prettier": "^2.2.1",
    "reflect-metadata": "^0.1.13",
    "tslib": "^2.1.0",
    "type-fest": "^0.21.3",
    "typescript": "^4.2.3"
  }
}
